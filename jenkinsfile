#!/usr/bin/env groovy
properties(
[
[
$class: 'GithubProjectProperty',
displayName: '',
projectUrlStr: 'https://github.com/Demo-project-saikadiyala1994/Practice_Repo_2.git'],
pipelineTriggers([upstream('master')])])
pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                echo 'make' 
            }
        }
        stage('Test'){
            steps {
                echo 'make check'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'make publish'
                build 'TestProject_1'
            }
        }
    }
}
