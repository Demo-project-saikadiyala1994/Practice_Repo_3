#!/usr/bin/env groovy
properties([[$class: 'GithubProjectProperty', displayName: '', projectUrlStr: 'https://github.com/Demo-project-saikadiyala1994/Practice_Repo_3/'], pipelineTriggers([upstream('Job_Organization/Practice_Repo_3/master')])])

pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                echo 'make changed...' 
            }
        }
        stage('Test'){
            steps {
                echo 'make checked....dgbnr.'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'make publish'
                build 'TestProject_1'
            }
        }
    }
}
